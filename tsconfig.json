{
  "compilerOptions": {
    "skipLibCheck": true,
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "target": "ES2020",
    "module": "Node16",
    "moduleResolution": "Node16",
    "noEmit": true,
    "allowImportingTsExtensions": true
  },
  "exclude": ["node_modules", "docs", "ts"]
}
